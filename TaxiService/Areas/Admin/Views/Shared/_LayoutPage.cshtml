    <script src="~/scripts/Flot_Plugins/curvedLines.js"></script>
    <script src="~/scripts/Flot_Plugins/jquery.flot.spline.min.js"></script>
    <script src="~/scripts/Flot_Plugins/jquery.flot.orderBars.js"></script>
