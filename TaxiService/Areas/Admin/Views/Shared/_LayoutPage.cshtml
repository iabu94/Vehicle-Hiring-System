    <script src="~/scripts/Date/date.js"></script>
    <!-- JQVMap -->
    <script src="~/scripts/JQVMap/jquery.vmap.js"></script>
    <script src="~/scripts/JQVMap/jquery.vmap.world.js"></script>
    <script src="~/scripts/JQVMap/jquery.vmap.sampledata.js"></script>
    <!-- bootstrap-daterangepicker -->
    <script src="~/scripts/Date/moment.min.js"></script>
